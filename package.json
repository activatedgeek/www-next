    "@fortawesome/react-fontawesome": "^0.2",
    "@iarna/toml": "^2.2",
    "@tailwindcss/typography": "^0.5",
    "date-fns": "^3.0",
    "next": "^14.0",
    "react": "^18.2",
    "react-dom": "^18.2",
    "tailwindcss": "^3.3",
    "vercel": "^33.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4",
    "eslint": "^8.56",
    "eslint-config-next": "^14.0",
    "github-slugger": "^2.0",
    "globby": "^14.0",
    "gray-matter": "^4.0",
    "next-mdx-remote": "^4.4",
    "postcss": "^8.4",
