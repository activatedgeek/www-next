{
  "name": "sanyamkapoor.com",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fmt": "prettier --write \"{api,app}/**/*.{js,css}\" *.config.js",
    "lint": "next lint",
    "start": "next start",
    "stork": "node api/stork.js && stork build --input public/index.toml --output public/index.st && rm public/index.toml",
