{
  "name": "sanyamkapoor.com",
  "version": "2.2.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fmt": "prettier --write \"{api,app}/**/*.{js,css}\" *.config.js",
    "lint": "next lint",
    "start": "next start",
    "stork": "node api/stork.js && stork build --input public/index.toml --output public/index.st && rm public/index.toml",
    "vercel": "vercel pull --yes --environment=production --token=${VERCEL_TOKEN} && vercel build --prod --token=${VERCEL_TOKEN} && vercel deploy --prebuilt --prod --token=${VERCEL_TOKEN}"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29",
    "@fortawesome/fontawesome-svg-core": "^6.5",
    "@fortawesome/free-brands-svg-icons": "^6.5",
