    "rehype-toc": "^3.0.2",
    "remark-emoji": "^4.0.0",
    "remark-gfm": "^3.0.1",
    "remark-hint": "^1.0.10",
    "remark-math": "^5.1.1",
    "shiki": "^0.14.4",
