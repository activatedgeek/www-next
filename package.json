    "@tailwindcss/typography": "^0.5.10",
    "date-fns": "^2.30.0",
    "next": "^13.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "vercel": "^32.2.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
