    "rehype-slug": "^6.0",
    "rehype-toc": "^3.0",
    "remark-emoji": "^4.0",
    "remark-gfm": "^3",
    "remark-hint": "^1.0",
    "remark-math": "^5",
    "shiki": "^0.14",
    "supports-color": "^9.4"
  }
}
